generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Job {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  clerkId   String
  company   String
  position  String
  location  String
  status    String
  mode      String
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}
